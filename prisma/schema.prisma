generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url = env("DATABASE_URL")
}

model Paste {
  id             String   @id @default(cuid())
  content        String
  expiresAt      DateTime?
  maxViews       Int?
  remainingViews Int?
  createdAt      DateTime @default(now())
}

